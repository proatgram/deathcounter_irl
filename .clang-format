  - CC
  - cpp
  - Cpp
  - CPP
  - c++
  - C++
  Language: Cpp
- BasedOnStyle: google
  CanonicalDelimiter: ''
  Delimiters:
  - pb
  - PB
  - proto
  - PROTO
  EnclosingFunctions:
  - EqualsProto
  - EquivToProto
