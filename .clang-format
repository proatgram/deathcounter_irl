- foreach
- Q_FOREACH
- BOOST_FOREACH
IncludeBlocks: Preserve
IncludeCategories:
- Priority: -1
  Regex: ^<[a-z_]+>
- Priority: 1
  Regex: ^<.+\.h(pp)?>
- Priority: 2
  Regex: ^<.*
- Priority: 3
  Regex: .*
IncludeIsMainRegex: ''
IncludeIsMainSourceRegex: ''
